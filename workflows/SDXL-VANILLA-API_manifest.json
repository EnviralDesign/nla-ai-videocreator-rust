{
  "adapter_type": "comfy_ui",
  "schema_version": 1,
  "name": "Vanilla SDXL Juggernaut",
  "output_type": "image",
  "workflow": {
    "workflow_path": "C:\\repos\\nla-ai-videocreator-rust\\workflows\\SDXL-VANILLA-API.json"
  },
  "inputs": [
    {
      "name": "seed",
      "label": "Seed",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 42,
      "bind": {
        "selector": {
          "class_type": "KSampler",
          "input_key": "seed",
          "title": "KSampler"
        }
      }
    },
    {
      "name": "positive_prompt",
      "label": "Positive Prompt",
      "input_type": {
        "type": "text"
      },
      "required": true,
      "default": "beautiful scenery nature glass bottle landscape, , purple galaxy bottle,",
      "ui": {
        "multiline": true,
        "advanced": false
      },
      "bind": {
        "selector": {
          "class_type": "CLIPTextEncode",
          "input_key": "text",
          "title": "prompt positive"
        }
      }
    },
    {
      "name": "negative_prompt",
      "label": "Negative Prompt",
      "input_type": {
        "type": "text"
      },
      "required": true,
      "default": "text, watermark",
      "ui": {
        "multiline": true,
        "advanced": false
      },
      "bind": {
        "selector": {
          "class_type": "CLIPTextEncode",
          "input_key": "text",
          "title": "prompt negative"
        }
      }
    },
    {
      "name": "width",
      "label": "Width",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 1024,
      "bind": {
        "selector": {
          "class_type": "EmptyLatentImage",
          "input_key": "width",
          "title": "Empty Latent Image"
        }
      }
    },
    {
      "name": "height",
      "label": "Height",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 1024,
      "bind": {
        "selector": {
          "class_type": "EmptyLatentImage",
          "input_key": "height",
          "title": "Empty Latent Image"
        }
      }
    },
    {
      "name": "steps",
      "label": "Steps",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 20,
      "bind": {
        "selector": {
          "class_type": "KSampler",
          "input_key": "steps",
          "title": "KSampler"
        }
      }
    }
  ],
  "output": {
    "selector": {
      "class_type": "SaveImage",
      "input_key": "images",
      "title": "Save Image"
    }
  }
}