{
  "adapter_type": "comfy_ui",
  "schema_version": 1,
  "name": "New Providerrr",
  "output_type": "image",
  "workflow": {
    "workflow_path": "C:\\repos\\nla-ai-videocreator-rust\\workflows\\sdxl_simple_example_API.json"
  },
  "inputs": [
    {
      "name": "noise_seed",
      "label": "Noise Seed",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 41,
      "bind": {
        "selector": {
          "class_type": "KSamplerAdvanced",
          "input_key": "noise_seed",
          "title": "KSampler (Advanced) - BASE"
        }
      }
    },
    {
      "name": "text",
      "label": "Text",
      "input_type": {
        "type": "text"
      },
      "required": true,
      "default": "hello",
      "ui": {
        "multiline": true,
        "advanced": false
      },
      "bind": {
        "selector": {
          "class_type": "CLIPTextEncode",
          "input_key": "text",
          "title": "Clip Positive Prompt"
        }
      }
    }
  ],
  "output": {
    "selector": {
      "class_type": "PreviewImage",
      "input_key": "images",
      "title": "preview image RESULT"
    }
  }
}