{
  "adapter_type": "comfy_ui",
  "schema_version": 1,
  "name": "SDXL Simple T2I",
  "output_type": "image",
  "workflow": {
    "workflow_path": "C:\\repos\\nla-ai-videocreator-rust\\workflows\\sdxl_simple_example_API.json"
  },
  "inputs": [
    {
      "name": "positive_prompt",
      "label": "Positive Prompt",
      "input_type": {
        "type": "text"
      },
      "required": true,
      "default": "masterpiece, high quality, absurdres",
      "ui": {
        "multiline": true,
        "advanced": false
      },
      "bind": {
        "selector": {
          "class_type": "CLIPTextEncode",
          "input_key": "text",
          "title": "Clip Positive Prompt"
        }
      }
    },
    {
      "name": "negative_prompt",
      "label": "Negative Prompt",
      "input_type": {
        "type": "text"
      },
      "required": true,
      "default": "bad quality, deformed, blurry",
      "ui": {
        "multiline": true,
        "advanced": false
      },
      "bind": {
        "selector": {
          "class_type": "CLIPTextEncode",
          "input_key": "text",
          "title": "Clip Negative Prompt"
        }
      }
    },
    {
      "name": "cfg",
      "label": "Cfg",
      "input_type": {
        "type": "number"
      },
      "required": false,
      "default": 7.5,
      "bind": {
        "selector": {
          "class_type": "KSamplerAdvanced",
          "input_key": "cfg",
          "title": "KSampler (Advanced) - BASE"
        }
      }
    },
    {
      "name": "steps",
      "label": "Steps",
      "input_type": {
        "type": "integer"
      },
      "required": false,
      "default": 20,
      "bind": {
        "selector": {
          "class_type": "KSamplerAdvanced",
          "input_key": "steps",
          "title": "KSampler (Advanced) - BASE"
        }
      }
    },
    {
      "name": "noise_seed",
      "label": "Noise Seed",
      "input_type": {
        "type": "integer"
      },
      "required": false,
      "default": 42,
      "bind": {
        "selector": {
          "class_type": "KSamplerAdvanced",
          "input_key": "noise_seed",
          "title": "KSampler (Advanced) - BASE"
        }
      }
    },
    {
      "name": "width",
      "label": "Width",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 1024,
      "bind": {
        "selector": {
          "class_type": "EmptyLatentImage",
          "input_key": "width",
          "title": "Empty Latent Image"
        }
      }
    },
    {
      "name": "height",
      "label": "Height",
      "input_type": {
        "type": "integer"
      },
      "required": true,
      "default": 1024,
      "bind": {
        "selector": {
          "class_type": "EmptyLatentImage",
          "input_key": "height",
          "title": "Empty Latent Image"
        }
      }
    }
  ],
  "output": {
    "selector": {
      "class_type": "PreviewImage",
      "input_key": "images",
      "title": "preview image RESULT"
    }
  }
}